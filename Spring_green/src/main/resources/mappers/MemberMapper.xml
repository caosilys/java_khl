<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.green.spring.dao.MemberDAO">
  <select id= "getMember" resultType="kr.green.spring.vo.MemberVO"> 
    select * from member where me_id = #{bd_id}
  </select>
  <insert id="sighUpMember">
  	insert into member value(#{member.me_id}, #{member.me_pw}, #{member.me_name}, #{member.me_gender}, #{member.me_birth}, #{member.me_address}, #{member.me_phone})
  </insert>
</mapper>
